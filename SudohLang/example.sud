function swap(a, b)
	temp <- a
	a <- b
	b <- temp

function sort(arr)
	n <- length(arr)
	for i <- 0 to n-2 do
		for j <- 0 to n-i-2 do
			if arr[j] > arr[j + 1] then
				swap(arr[j], arr[j + 1])

